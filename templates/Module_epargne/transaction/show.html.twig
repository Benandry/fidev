{{ include('header.html.twig') }}

{% block body %}
    <h1>Transaction</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ transaction.id }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ transaction.Description }}</td>
            </tr>
            <tr>
                <th>PieceComptable</th>
                <td>{{ transaction.PieceComptable }}</td>
            </tr>
            <tr>
                <th>DateTransaction</th>
                <td>{{ transaction.DateTransaction ? transaction.DateTransaction|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Montant</th>
                <td>{{ transaction.Montant }}</td>
            </tr>
            <tr>
                <th>Papeterie</th>
                <td>{{ transaction.papeterie }}</td>
            </tr>
            <tr>
                <th>Commission</th>
                <td>{{ transaction.commission }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_transaction_index') }}">back to list</a>

    <a href="{{ path('app_transaction_edit', {'id': transaction.id}) }}">edit</a>

    {{ include('Module_epargne/transaction/_delete_form.html.twig') }}
{% endblock %}

{{ include('footer.html.twig') }}

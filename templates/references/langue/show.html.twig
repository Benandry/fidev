{{ include('header.html.twig') }}

{% block body %}
    <h1>Langue</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ langue.id }}</td>
            </tr>
            <tr>
                <th>Langue</th>
                <td>{{ langue.langue }}</td>
            </tr>
            <tr>
                <th>Pays</th>
                <td>{{ langue.pays }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_langue_index') }}" class="btn btn-primary btn-sm">Voir  listes </a>

    <a href="{{ path('app_langue_edit', {'id': langue.id}) }}" class="btn btn-warning btn-sm" >Modifier</a>

{% endblock %}




         <!-- The Modal -->
         <div class="modal fade" id="myModal">
             <div class="modal-dialog modal-dialog-centered">
                <div class="modal-content">
                 
                     <!-- Modal Header -->
                     <div class="modal-header bg-primary">
                         <h4 class="modal-title text-center"> Configuration epargne </h4>
                         <button type="button" class="close" data-dismiss="modal">&times;</button>
                     </div>
                        {{ form_start(form) }}
                            <!-- Modal body -->
                            <div class="modal-body">
                                <div class="row">
                                    <div class="col-sm-6">
                                        {{ form_row(form.produitEpargne) }}
                                        {{ form_row(form.IsNegatif) }}
                                        {{ form_row(form.deviseutiliser) }}
                                        {{ form_row(form.nbrjInactif) }}
                                
                                        {{ form_row(form.nbMinRet) }}
                                        {{ form_row(form.NbrJrMaxDep) }}
                                    </div>
                                    <div class="col-sm-6">
                                        {{ form_row(form.ageMinCpt) }}
                                        {{ form_row(form.fraisTenuCpt) }}       
                                        {{ form_row(form.commissionRetCash) }}
                                        {{ form_row(form.commissionTransf) }}
                                
                                        {{ form_row(form.fraisFermCpt) }}
                                        {{ form_row(form.soldeouvert) }}
                                    </div>
                                </div>
                            </div>
                            
                            <!-- Modal footer -->
                            <div class="modal-footer">
                                <button class="btn btn-success" >{{ button_label|default('Enregistrer') }}</button>
                                <a href="{{ path('app_config_ep_index') }}" class="btn btn-success"><i class="fa fa-eye"></i> Voir Liste</a>
                            </div>
                        {{ form_end(form) }}
                     
                </div>
             </div>
         </div>
         {# /***********Fin modal *********** #}
   {% block javascript %}

        <script>
                $(document).ready(function(){
                    $(window).on('load',()=>{
                        $('#myModal').modal('show')
                    })
                });
        </script>
    {% endblock %}

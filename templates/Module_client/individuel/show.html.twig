{{ include('header.html.twig') }}

{% block body %}

<section style="background-color: #eee;" style="color:black;">
  <div class="container py-5">
    <div class="row">
      <div class="col">
        <nav aria-label="breadcrumb" class="bg-light rounded-3 p-3 mb-4">
          <ol class="breadcrumb mb-0">
            <li class="breadcrumb-item"><a href="#"></a></li>
            <li class="breadcrumb-item"><a href="#"></a></li>
            <li class="breadcrumb-item active" aria-current="page"></li>
          </ol>
        </nav>
      </div>
    </div>

    <div class="row">
      <div class="col-lg-4">
        <div class="card mb-4">
          <div class="card-body text-center">
            <img src="{{ asset('uploads/photo/' ~ individuelclients.getPhoto()  ) }}" alt="avatar"
              class="rounded-circle img-fluid" style="width: 150px;height:150px;border-radius:100%;">
            <h5 class="my-3" style="color:black;"><b>{{ individuelclients.nomClient }}</b></h5>
            <h5 class="my-3" style="color:black;"><b>{{ individuelclients.prenomClient }}</b></h5>
            <h5 class="my-3" style="color:black;"><b> {{ individuelclients.codeclient }}</b></h5>
            <p class="text-muted mb-1"></p>
            <p class="text-muted mb-4"></p>
            <div class="d-flex justify-content-center mb-2">
            </div>
          </div>
        </div><br><br><br>
        <div class="card mb-4 mb-lg-0">
          <div class="card-body p-0">
            <ul class="list-group list-group-flush rounded-3">
              <li class="list-group-item d-flex justify-content-between align-items-center p-3">
                <i class=""></i>
                <p class="mb-0"></p>
              </li>
              <li class="list-group-item d-flex justify-content-between align-items-center p-3">
                <i class="" style="color:black;">Etat Civile</i>
                <p class="">{{etatciviles.etatcivile}}</p>
              </li>
              <li class="list-group-item d-flex justify-content-between align-items-center p-3">
                <i class="" style="color:black;">Nombre enfant</i>
                <p class="">{{individuelclients.nbEnfant}}</p>
              </li>
              <li class="list-group-item d-flex justify-content-between align-items-center p-3">
                <i class="" style="color:black;">Nombre personne en charge</i>
                <p class="">{{individuelclients.nbPersonneCharge}}</p>
              </li>
              <li class="list-group-item d-flex justify-content-between align-items-center p-3">
                <i class="" style="color: #3b5998;"></i>
                <p class=""></p>
              </li>
            </ul>
          </div>
        </div>
      </div>
      <div class="col-lg-8">
        <div class="card mb-4 alert alert-Primary">
          <div class="card-body">
            <div class="row">
              <div class="col-sm-3">
                <p class="mb-0" style="color:black;">Carte d'identite National</p>
              </div>
              <div class="col-sm-9">
                <p class="text-muted mb-0">{{ individuelclients.cin }}</p>
              </div>
            </div>
            <hr>
            <div class="row">
              <div class="col-sm-3">
                <p class="mb-0" style="color:black;">Adresse</p>
              </div>
              <div class="col-sm-9">
                <p class="text-muted mb-0">{{ individuelclients.adressePhysique }}</p>
              </div>
            </div>
            <hr>
            <div class="row">
              <div class="col-sm-3">
                <p class="mb-0" style="color:black;">Numero Telephone</p>
              </div>
              <div class="col-sm-9">
                <p class="text-muted mb-0">{{ individuelclients.numeroMobile }}</p>
              </div>
            </div>
            <hr>
            <hr>
            <div class="row">
              <div class="col-sm-3">
                <p class="mb-0" style="color:black;">Proffession</p>
              </div>
              <div class="col-sm-9">
                <p class="text-muted mb-0">{{ individuelclients.profession }}</p>
              </div>
            </div>
          </div>
        </div><br><br>
        <div class="row">
          <div class="col-md-6">
            <div class="card mb-4 mb-md-0">
              <div class="card-body">
                <p class="mb-4"><span class="text-primary font-italic me-1" style="color:black;">Information Adresse :</span> 
                </p>
                <p class="mb-1" style="color:black;">Commune :</p>
                <div class="progress rounded">
                <p class="mb-1">{{Commune.Nomcommune}}</p>
                </div>
                <p class="mt-4 mb-1" style="color:black;">Code Commune :</p>
                 <div class="progress rounded" >
                        <p class="mt-4 mb-1">{{Commune.CodeCommune}}</p>
                </div>
                 <p class="mt-4 mb-1" style="color:black;">Adresse Physique :</p>
                <div class="progress rounded">
                 <p class="mt-4 mb-1">{{individuelclients.adressePhysique}}</p>
                </div>

              </div>
            </div>
          </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>

    </ul>

   
{% endblock %}

